meta {
  name: Create Match
  type: http
  seq: 1
}

post {
  url: {{base_url}}/api/games/{{game_id}}/matches
  body: json
  auth: inherit
}

body:json {
  {
    "scores": [
      {
        "user_id": "user_id_1",
        "score": 11
      },
      {
        "user_id": "user_id_2",
        "score": 25
      }
    ]
  }
}

vars:pre-request {
  game_id: {{game_id}}
}

settings {
  encodeUrl: true
  timeout: 0
}
